<?xml version="1.0" encoding="UTF-8"?>
<cross-domain-policy xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.adobe.com/xml/schemas/PolicyFile.xsd">
	<site-control permitted-cross-domain-policies="master-only"/>
	<allow-access-from domain="*.wikihow.com" />
	<allow-access-from domain="*.wikihow.vn" />
	<allow-access-from domain="*.wikihow.jp" />
	<allow-access-from domain="*.wikihow.it" />
	<allow-access-from domain="*.wikihow.cz" />
	<allow-access-from domain="*.wikihow.com.tr" />
	<allow-access-from domain="*.wikidiy.com" />
	<allow-access-from domain="*.wikidogs.com" />
	<allow-http-request-headers-from domain="*.wikihow.com" headers="*" secure="true"/>
	<allow-http-request-headers-from domain="*.wikihow.vn" headers="*" secure="true"/>
	<allow-http-request-headers-from domain="*.wikihow.jp" headers="*" secure="true"/>
	<allow-http-request-headers-from domain="*.wikihow.it" headers="*" secure="true"/>
	<allow-http-request-headers-from domain="*.wikihow.cz" headers="*" secure="true"/>
	<allow-http-request-headers-from domain="*.wikihow.com.tr" headers="*" secure="true"/>
</cross-domain-policy>
